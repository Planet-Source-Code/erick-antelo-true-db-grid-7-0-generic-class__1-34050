Dim grid As clsGrilla

Private Sub Form_Load()
Dim adoRs As ADODB.Recordset
Dim adoCn As ADODB.Connection
Dim strPath As String

'Open connection
Set adoCn = New ADODB.Connection
strPath = App.Path & "\Sample.mdb"
adoCn.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=" & strPath & ";Persist Security Info=False"
adoCn.CursorLocation = adUseClient
adoCn.Open strPath

'Open recordset
Set adoRs = New ADODB.Recordset
adoRs.Open "customer", adoCn, adOpenKeyset, adLockOptimistic

Set Me.TDBGrid1.DataSource = adoRs

'set class info
Set grid = New clsGrilla
Set grid.Grilla = Me.TDBGrid1
Set grid.rs = adoRs

End Sub

Private Sub Form_Unload(Cancel As Integer)
    Set adoRs = Nothing
    Set adoCn = Nothing
    
End Sub

